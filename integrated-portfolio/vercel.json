{
  "functions": {
    "api/index.js": {
      "memory": 1024
    },
    "api/youtube.js": {
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/youtube/uploads",
      "dest": "/api/youtube.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
