{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/vite.config.js",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
